{
  "name": "headerbar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "@dhis2/ui-core": "^4.1.1",
    "@dhis2/ui-widgets": "^2.0.5",
    "@dhis2/app-runtime": "^2.0.4",
    "@dhis2/d2-i18n": "^1.0.6",
    "d2": "^31.8.0",
    "styled-jsx": "^3.2.4"
  },
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:react && npm run build:clean",
    "build:react": "react-scripts build",
    "build:clean": "rm build/asset-manifest.json && rm build/service-worker.js && rm build/precache-manifest*",
    "build:zip": "(rm -f headerbar.zip && cd cleanbuild && zip ../headerbar.zip -r .)"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
